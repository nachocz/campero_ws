<?xml version="1.0"?>
<package format="2">
  <name>campero_gazebo</name>
  <version>0.0.0</version>
  <description>The campero_gazebo package</description>

  <maintainer email="aarnal@robotnik.es">Alejandro Arnal</maintainer>
  <maintainer email="jgomez@robotnik.es">Jose GÃ³mez</maintainer>
  <maintainer email="mmillet@robotnik.es">Marta Miller</maintainer>


  <license>BSD</license>


  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>gazebo_ros</build_depend>
  <build_depend>campero_control</build_depend>  
  <build_depend>campero_robot_control</build_depend>  
